{
  "presets": ["next/babel"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "alias": {
          "components": "./components",
          "utils": "./utils",
          "constants": "./utils/constants",
          "styles":"./styles",
          "bindRedux":"./redux/store",
          "actions":"./redux/actions"          
        }
      }
    ],
    ["import", { "libraryName": "antd" }]
  ],
  "ignore": []
}